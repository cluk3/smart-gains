    "web": "expo start --web",
    "seed:sync": "pnpx @snaplet/seed sync",
    "seed:gen": "pnpx tsx seed.ts > supabase/seed.sql",
    "supabase:start": "supabase start",
    "supabase:typegen": "supabase gen types typescript --project-id txjqycwkbvispusmunks > supabase/types.ts"
